{
    "chainer":{
        "in":[
            "x_topics_with_id",
            "x_da_topics_with_id",
            "x_da_intents_with_id",
            "x_emo_with_id",
            "x_sentiment_with_id",
            "x_toxic_with_id",
            "x_factoid_with_id"
        ],
        "pipe":[
            {
                "class_name":"multitask_pal_bert_preprocessor",
                "in":[
                    "x_topics_with_id",
                    "x_da_topics_with_id",
                    "x_da_intents_with_id",
                    "x_emo_with_id",
                    "x_sentiment_with_id",
                    "x_toxic_with_id",
                    "x_factoid_with_id"
                ],
                "out":[
                    "task_id",
                    "x_topics",
                    "x_da_topics",
                    "x_da_intents",
                    "x_emo",
                    "x_sentiment",
                    "x_toxic",
                    "x_factoid"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":64,
                "in":[
                    "x_topics"
                ],
                "out":[
                    "bert_features_x_topics"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":64,
                "in":[
                    "x_da_topics"
                ],
                "out":[
                    "bert_features_x_da_topics"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":64,
                "in":[
                    "x_da_intents"
                ],
                "out":[
                    "bert_features_x_da_intents"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":32,
                "in":[
                    "x_emo"
                ],
                "out":[
                    "bert_features_x_emo"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":32,
                "in":[
                    "x_factoid"
                ],
                "out":[
                    "bert_features_x_factoid"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":32,
                "in":[
                    "x_sentiment"
                ],
                "out":[
                    "bert_features_x_sentiment"
                ]
            },
            {
                "class_name":"torch_transformers_preprocessor",
                "vocab_file":"bert-base-uncased",
                "max_seq_length":32,
                "in":[
                    "x_toxic"
                ],
                "out":[
                    "bert_features_x_toxic"
                ]
            },
            {
                "id":"multitask_pal_bert",
                "class_name":"multitask_pal_bert",
                "pretrained_bert":"{PRETRAINED_BERT}/pytorch_model.bin",
                "optimizer_parameters":{
                    "lr":4e-5
                },
                "gradient_accumulation_steps":"{GRADIENT_ACC_STEPS}",
                "steps_per_epoch":"{STEPS_PER_EPOCH}",
                "learning_rate_drop_patience":2,
                "learning_rate_drop_div":2.0,
                "return_probas":true,
                "log_model_summary":false,
                "save_path":"{MODELS_PATH}/model",
                "load_path":"{MODELS_PATH}/model",
                "tasks":{
                    "topics":{
                        "n_classes":22
                    },
                    "da_topics":{
                        "n_classes":11
                    },
                    "da_intents":{
                        "n_classes":11
                    },
                    "emo":{
                        "n_classes":7
                    },
                    "sentiment":{
                        "n_classes":3
                    },
                    "toxic":{
                        "n_classes":8
                    },
                    "factoid":{"n_classes": 2}
                },
                "in_distribution":{
                    "topics":1,
                    "da_topics":1,
                    "da_intents":1,
                    "factoid":1,
                    "emo":1,
                    "toxic":1,
                    "sentiment":1
                },
                "in":[
                    "task_id",
                    "bert_features_x_topics",
                    "bert_features_x_da_topics",
                    "bert_features_x_da_intents",
                    "bert_features_x_emo",
                    "bert_features_x_sentiment",
                    "bert_features_x_toxic",
                    "bert_features_x_factoid"
                ],
                "in_y_distribution":{
                    "topics":1,
                    "da_topics":1,
                    "da_intents":1,
                    "factoid":1,
                    "emo":1,
                    "toxic":1,
                    "sentiment":1
                },
                "in_y":[
                    "y_topics",
                    "y_da_topics",
                    "y_da_intents",
                    "y_emo",
                    "y_sentiment",
                    "y_toxic",
                    "y_factoid"
                ],
                "out":[
                    "y_topics_pred",
                    "y_da_topics_pred",
                    "y_da_intents_pred",
                    "y_emo_pred",
                    "y_sentiment_pred",
                    "y_toxic_pred",
                    "y_factoid_pred"
                ]
            },
            {
                "in": [
                    "y_topics_pred"
                ],
                "out": [
                    "y_topics_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            },
            {
                "in": [
                    "y_da_topics_pred"
                ],
                "out": [
                    "y_da_topics_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            },
            {
                "in": [
                    "y_da_intents_pred"
                ],
                "out": [
                    "y_da_intents_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            },
            {
                "in": [
                    "y_emo_pred"
                ],
                "out": [
                    "y_emo_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            },
            {
                "in": [
                    "y_sentiment_pred"
                ],
                "out": [
                    "y_sentiment_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            },
            {
                "in": [
                    "y_toxic_pred"
                ],
                "out": [
                    "y_toxic_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            },
            {
                "in": [
                    "y_factoid_pred"
                ],
                "out": [
                    "y_factoid_pred_ids"
                ],
                "class_name": "proba2labels",
                "max_proba": true
            }
        ],
        "out":[
            "y_topics_pred_ids",
            "y_da_topics_pred_ids",
            "y_da_intents_pred_ids",
            "y_emo_pred_ids",
            "y_sentiment_pred_ids",
            "y_toxic_pred_ids",
            "y_factoid_pred_ids"
        ]
    },
    "metadata":{
        "variables":{
            "ROOT_PATH":"/home/karpov.d",
            "DOWNLOADS_PATH":"{ROOT_PATH}/downloads",
            "PRETRAINED_BERT":"{ROOT_PATH}/pretrained_bert",
            "MODELS_PATH":"{ROOT_PATH}/.deeppavlov/multitask_models",
            "GLUE_CSV":"{ROOT_PATH}/GLUE/Data",
            "NUM_TRAIN_EPOCHS":10,
            "STEPS_PER_EPOCH":100000,
            "GRADIENT_ACC_STEPS":2
        },
        "download":[
            {
                "url":"https://huggingface.co/bert-base-uncased/resolve/main/pytorch_model.bin",
                "subdir":"{PRETRAINED_BERT}"
            }
        ]
    }
}
