{
  "chainer": {
    "in": [
      "utterances_batch",
      "history_batch",
      "states_batch"
    ],
    "out": [
      "best_answers_batch"
    ],
    "pipe": [
      {
        "class_name": "aiml_skill",
        "id": "my_aiml_skill",
        "path_to_aiml_scripts": "skills/aiml/Free-AIML",
        "positive_confidence": 0.96,
        "null_response": "I don't know",
        "null_confidence": 0.2,
        "in": [
          "utterances_batch",
          "history_batch",
          "states_batch"
        ],
        "out": [
          "aiml_responses_batch",
          "aiml_confidences_batch",
          "aiml_output_states_batch"
        ]
      },
      {
        "class_name": "rule_based_selector",
        "aiml_null_response": "#my_aiml_skill.null_response",
        "in": [
          "utterances_batch",
          "aiml_responses_batch"
        ],
        "out": [
          "best_answers_batch"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "imports": [
      "skill_selectors.alexa_skill_selectors.rule_based_selector"
    ],
    "labels": {
      "server_utils": "RuleBasedSkillSelector"
    },
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/aiml_skill.txt"
    ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/aiml_skill/aiml_scripts.tar.gz",
        "subdir": "{DOWNLOADS_PATH}"
      }
    ]
  }
}